.bookInfo .opacity{
	display: none;
	position: absolute;
	top:0;
	left: 0;
	background: #eee;
	opacity:0.7;
	height: 100%;
	width: 100%;
	z-index:1;
}
.bookInfo .main_comment{
	display: none;
	position: absolute;
	-webkit-animation:append .2s ease-out;
	background-color: #fff;
	top: 1em;
	z-index: 2;
	left: 1em;
	padding: .2em 1em 1em;
	right: 1em;
	border-radius: 5px;
	line-height: 1.5em;
	font-size: .9em;
}
@-webkit-keyframes append{
	0%{-webkit-transform:scale(0,0)}
	100%{-webkit-transform:scale(100%,100%)}
}
@-webkit-keyframes shut{
	0%{-webkit-transform:scale(100%,100%)}
	100%{-webkit-transform:scale(0,0)}
}
